{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "get-path",
  "title": "Get Path",
  "description": "Extension to get current path from Finder.",
  "icon": "command-icon.png",
  "author": "duoc_nguyen",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "get-path",
      "title": "Get Path",
      "subtitle": "Get path of current selected item",
      "description": "You need to select a file first",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.56.3",
    "run-applescript": "^6.1.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
